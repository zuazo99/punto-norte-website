---
import ContentSection from "../content-section.astro";
import FutureEvents from "./future-events.astro";
import { GET } from "../../pages/api/future-events";
export const prerender = false;
const futureEvents = await GET().then((res) => res.json());
---

<ContentSection title="Eventos" id="events">
  <FutureEvents events={futureEvents.eventCollection.items} />
</ContentSection>
